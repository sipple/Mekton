<table>
  <thead class="character-table-header shaded">
    <tr>
      <th>Equipment</th>
      <th>Wgt</th>
      <th>Qty</th>
      <th>Cost</th>
    </tr>
  </thead>
  <tbody>
    <% local_assigns[:character_equipments].each do |equipment| %>
        <%= render :partial => '/character_equipments/character_equipment_line', :locals => {:character_equipment => equipment} %>
    <% end %>
  <tr id="character_equipments-add">
     <td><%= image_tag("add.png", :alt=>"Add Equipment", :class=>"icon add") %></td>
  </tr>
  </tbody>
</table>